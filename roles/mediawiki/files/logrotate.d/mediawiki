/var/log/mediawiki/*.log {
	daily
	missingok
	rotate 7
	compress
	delaycompress
	notifempty
	su mediawiki mediawiki
	create 660 mediawiki mediawiki
}

/var/log/mediawiki/debuglogs/*.log {
	daily
	missingok
	rotate 7
	compress
	delaycompress
	notifempty
	su mediawiki mediawiki
	create 660 mediawiki mediawiki
}
